{
	"name": "trello",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "vite --host 0.0.0.0 --port 3000",
		"build": "tsc && vite build",
		"tslint": "tsc -p tsconfig.json --noEmit",
