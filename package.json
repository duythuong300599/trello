		"react": "^18.2.0",
		"react-beautiful-dnd": "^13.1.1",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.12.1",
		"sass": "^1.62.1",
		"classnames": "^2.3.2",
		"vite-tsconfig-paths": "^4.2.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.6",
		"@commitlint/config-conventional": "^17.6.6",
